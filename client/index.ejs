  <script src="hospital_auth/hospital_auth_controller.js"></script>
  <script src="hospital_profile/profile_service.js"></script>
  <script src="hospital_profile/profile_controller.js"></script>
  <script src="stock/stock_controller.js"></script>
  <script src="donor_auth/donor_auth_controller.js"></script>
  <script src="donor_auth/donor_auth_service.js"></script>
  <script src="calendar/calendar_service.js"></script>
  <script src="calendar/calendar_controller.js"></script>
